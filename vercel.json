{
    "version": 2,
    "builds": [
        {
            "src": "api/webhook.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "api/webhook.py"
        }
    ],
    "env": {
        "TELEGRAM_TOKEN": "@telegram_token",
        "GEMINI_API_KEY": "@gemini_api_key",
        "GOOGLE_SERVICE_ACCOUNT_JSON": "@google_service_account_json"
    }
}